{
	"extends": ["eslint:recommended", "plugin:prettier/recommended"],
	"plugins": ["prettier", "tailwindcss"],
	"env": {
		"browser": true,
		"node": true,
		"jest": true,
		"es6": true
	},
	"parserOptions": {
		"sourceType": "module",
		"ecmaVersion": "latest"
	},
	"rules": {
		"prettier/prettier": ["error", {"endOfLine": "auto"}]
	}
}
