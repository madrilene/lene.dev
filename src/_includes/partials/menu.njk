<nav id="mobile-menu" data-id="nav-items" aria-label="Primary navigaton">
  <ul role="list" class="nav__list">
    {% for item in navigation.items %}

    <li>
      <a href="{{ item.url }}" {{ helpers.getLinkActiveState(item.url, page.url) | safe }}
        ><span class="inline-block p-1 md:p-0">{{ item.text }}</span></a
      >
    </li>

    {% endfor %}
  </ul>
</nav>
