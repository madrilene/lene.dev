<nav id="mobile-menu" class="z-20" data-id="nav-items" aria-label="Primary navigaton">
  <ul role="list" class="nav__list">
    {% for item in navigation.items %}

    <li>
      <a
        class="nav"
        href="{{ item.url }}"
        {{
        helpers.getLinkActiveState(item.url,
        page.url)
        |
        safe
        }}
        >{{ item.text }}</a
      >
    </li>

    {% endfor %}
  </ul>
</nav>
