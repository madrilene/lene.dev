{
	"name": "lene-dev",
	"version": "0.2.0",
	"description": "My developer page. Made with Eleventy, ESBuild and Tailwind CSS as utilities for CUBE CSS",
	"scripts": {
		"clean": "rimraf dist",
		"dev:postcss": "postcss src/_assets/css/ --dir dist/_assets/css --watch --verbose",
		"dev:scripts": "esbuild src/_assets/scripts/app.js --bundle --watch --outfile=dist/_assets/app.min.js",
		"dev:cube": "node css-props-generator.js",
		"dev:11ty": "eleventy --serve --watch",
		"build:postcss1": "NODE_ENV=production postcss src/_assets/css/global.css -o src/_includes/global.min.css",
		"build:postcss2": "NODE_ENV=production postcss src/_assets/css/cube.css -o src/_includes/cube.min.css",
		"build:11ty": "cross-env ELEVENTY_ENV=production eleventy",
		"build:scripts": "esbuild src/_assets/scripts/app.js --bundle --minify --outfile=dist/_assets/app.min.js",
		"start": "run-p dev:*",
		"build": "run-s clean build:*"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/madrilene/lene-dev"
	},
	"author": "Lene Saile",
	"license": "ISC",
	"devDependencies": {
		"@11ty/eleventy": "^2.0.0-canary.11",
		"@11ty/eleventy-img": "^2.0.1",
		"@11ty/eleventy-plugin-syntaxhighlight": "^4.0.0",
		"@iktakahiro/markdown-it-katex": "^4.0.1",
		"@netlify/plugin-a11y": "^1.0.0-alpha.6",
		"@toycode/markdown-it-class": "^1.2.4",
		"autoprefixer": "^10.4.0",
		"clean-css": "^5.3.0",
		"cross-env": "^7.0.3",
		"dayjs": "^1.10.7",
		"esbuild": "^0.14.5",
		"eslint": "^8.4.1",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-tailwindcss": "^3.5.0",
		"glob": "^8.0.3",
		"html-minifier": "^4.0.0",
		"lodash": "^4.17.21",
		"markdown-it": "^13.0.1",
		"markdown-it-anchor": "^8.4.1",
		"markdown-it-link-attributes": "^4.0.0",
		"markdown-it-prism": "^2.2.1",
		"netlify-plugin-11ty": "^1.1.0",
		"npm-run-all": "^4.1.5",
		"outdent": "^0.8.0",
		"postcss": "^8.4.5",
		"postcss-cli": "^9.1.0",
		"postcss-import": "^14.1.0",
		"prettier": "^2.5.1",
		"rimraf": "^3.0.2",
		"sanitize-html": "^2.6.1",
		"slugify": "^1.6.5",
		"svg-sprite": "^1.5.3",
		"tailwindcss": "^3.0.16",
		"vinyl": "^2.2.1"
	}
}
